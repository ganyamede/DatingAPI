{
  "swagger": "2.0",
  "info": {
    "title": "My API",
    "description": "API для демонстрации Swagger UI",
    "version": "1.0.0"
  },
  "paths": {
    "/api/sign": {
      "post": {
        "summary": "Регистрация пользователя",
        "responses": {
          "200": {
            "description": "Успешная регистрация"
          }
        }
      }
    },
    "/api/register": {
      "post": {
        "summary": "Регистрация нового пользователя",
        "responses": {
          "200": {
            "description": "Успешная регистрация"
          }
        }
      }
    },
    "/api/refresh": {
      "post": {
        "summary": "Обновление токена",
        "responses": {
          "200": {
            "description": "Токен обновлен"
          }
        }
      }
    }
  }
}
